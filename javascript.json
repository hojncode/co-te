{
    "Console Log": {
        "prefix": "cl",
        "body": [
            "console.log($1);"
        ],
        "description": "Console log"
    },
    "Arrow Function": {
        "prefix": "af",
        "body": [
            "const $1 = ($2) => {",
            "    $3",
            "};"
        ],
        "description": "Arrow function"
    },
    "For Loop": {
        "prefix": "fl",
        "body": [
            "for (let i = 0; i < $1; i++) {",
            "    $2",
            "}"
        ],
        "description": "For loop"
    },
    "For Each": {
        "prefix": "fe",
        "body": [
            "$1.forEach((item) => {",
            "    $2",
            "});"
        ],
        "description": "forEach loop"
    },
    "Map Function": {
        "prefix": "map",
        "body": [
            "$1.map((item) => {",
            "    return $2",
            "});"
        ],
        "description": "Map function"
    }
} 